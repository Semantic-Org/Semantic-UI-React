    "json-loader": "^0.5.3",
    "karma": "^1.3.0",
    "karma-cli": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^3.1.2",
    "node-sass": "^3.12.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "react": "^15.3.1",
    "react-ace": "^3.7.0",
