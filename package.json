{
  "name": "semantic-ui-theme-builder",
  "version": "0.1.0",
  "description": "A theme builder for Semantic UI components",
  "scripts": {
    "reset": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install",
    "start:light": "THEME=child-theme-light cross-env NODE_ENV=development gulp --series start:docs",
    "start:dark": "THEME=child-theme-dark cross-env NODE_ENV=development gulp --series start:docs",
    "build:themes": "rm -rf themes/dist && node themes/config/build.js",
    "build:docs": "rm -rf docs/dist && cross-env NODE_ENV=production gulp build:docs",
    "serve:docs": "serve docs/dist",
    "deploy:docs": "gulp deploy:docs",
    "eslint": "eslint --ext .js ./docs ./themes .src/externals",
    "eslint:fix": "eslint --fix --ext .js ./docs ./themes .src/externals"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-env-standalone": "7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@babel/standalone": "^7.1.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@mdx-js/loader": "^0.15.5",
    "@semantic-ui-react/event-stack": "^3.0.0",
    "@types/react": "^16.4.14",
    "anchor-js": "^4.1.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-filter-imports": "^2.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-handled-props": "^1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "babel-plugin-universal-import": "^2.0.2",
    "bfj": "^6.1.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "copy-webpack-plugin": "^4.6.0",
