{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "enzyme": false,
    "expect": false,
    "mount": false,
    "shallow": false,
    "render": false
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "jsx-a11y/alt-text": 0,
    "jsx-a11y/tabindex-no-positive": 0
  }
}
